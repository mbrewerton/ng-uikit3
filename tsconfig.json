{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "module": "esnext",
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2015",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "uk-button": [
        "dist/uk-button"
      ],
      "uk-button/*": [
        "dist/uk-button/*"
      ],
      "uk-core": [
        "dist/uk-core"
      ],
      "uk-core/*": [
        "dist/uk-core/*"
      ],
      "@mbrewerton/ng-uikit3": [
        "dist/mbrewerton/ng-uikit3"
      ],
      "@mbrewerton/ng-uikit3/*": [
        "dist/mbrewerton/ng-uikit3/*"
      ]
    }
  },
  "angularCompilerOptions": {
    "fullTemplateTypeCheck": true,
    "strictInjectionParameters": true
  }
}